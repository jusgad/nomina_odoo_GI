        <!-- Tipos de Contrato -->
        <record id="hr_contract_type_indefinido" model="hr.contract.type">
            <field name="name">Término Indefinido</field>
            <field name="code">INDEF</field>
            <field name="country_id" ref="base.co"/>
        </record>

        <record id="hr_contract_type_fijo" model="hr.contract.type">
            <field name="name">Término Fijo</field>
            <field name="code">FIJO</field>
            <field name="country_id" ref="base.co"/>
